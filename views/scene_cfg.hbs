<div class="clearfix">
    <div class="btn-group">
        <button id="sample_editable_1_new" class="btn green">添加<i class="icon-plus"></i>
        </button>
        <button class="btn" type="button" id="batch_del">批量删除</button>
    </div>
</div>
<table class="table table-striped table-hover table-bordered sourceType">
    <thead>
    <tr class="table-title">
        <th cond="delete_all"><input type="checkbox" id="select_all"/></th>
        <th cond="databaseType">业务场景<span style="margin-left:10px"><i class="icon-chevron-up"></i></span></th>
        <th>编辑</th>
        <th>删除</th>
    </tr>
    </thead>
    <tbody>
    {{#if scene}}
        {{#each scene}}
            <tr>
                <td class='text-center'><input  type="checkbox" value={{id}}></td>
                <td class='text-center' style='display:none'>{{id}}</td>
                <td class='text-center'>{{sourceType}}</td>
                <td class='text-center'>
                    <a class='edit'  href='javascript:;'>编辑</a></td>
                <td class='text-center'>
                    <a class='delete'  href='javascript:;' >删除</a></td>
            </tr>
        {{/each}}
    {{else}}
        <tr>
            <td colspan="4" class='text-center'>暂无数据</td>
        </tr>
    {{/if}}
    </tbody>
</table>
<input type="hidden" id="del" value="delSource"/>
<div class="pagination pagination-mini" style="float: right; margin-right: 30px; margin-bottom: 20px;">
    <div class="M-box" totalPage={{totalPage}} page={{page}}></div>
</div>
<!--START TABLR BACKGROUNDGREY-->
<div class="tablebg-grey"></div>
<!--END TABLR BACKGROUNDGREY-->
<!--START TABLR ADD MEW-->
<div class="add-new add-new-sourceType" style="display:none;">
    <h4><i class="icon-edit"></i>添加业务场景<img src="image/portlet-remove-icon-white.png"></h4>
    <form>
        <p class="input-text">
            <label>业务场景</label>
            <input type="text">
            <span>不能为空</span></p>
        <p class="btn-form"><span>
                    <input type="button" value="保 存" id="save">
                    </span></p>
    </form>
</div>
<!--END TABLR ADD MEW-->

<!--START TABLR CHANGE-->
<div class="change-message" style="display:none;">
    <h4><i class="icon-edit"></i>业务场景编辑<img src="image/portlet-remove-icon-white.png"></h4>
    <form>
        <p class="input-text">
            <label>业务场景</label>
            <input type="hidden" />
            <input type="text" />
            <span>不能为空</span></p>
        <p class="btn-form">
            <input type="button" value="保 存" id="updata_save">
            <input type="button" value="取 消" class="cancel">
        </p>
        <input type="hidden" name="originalData" id="originalData" value="">
    </form>
</div>
<!--END TABLR CHANGE-->
</div>
</div>
<!-- END EXAMPLE TABLE PORTLET-->
</div>
</div>
<!-- END PAGE CONTENT -->
</div>
<!-- END PAGE CONTAINER-->
</div>
<!-- END PAGE -->
{{>common_js}}
<script src="js/jquery.pagination.js"></script>
<script src="js/scene_cfg.js"></script>