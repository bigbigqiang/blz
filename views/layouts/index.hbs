    <link rel="stylesheet" href="/css/index.css">
    <link rel="stylesheet" href="/css/icon.css">
    {{{_sections.head}}}
    {{!END BEGIN PAGE LEVEL STYLES }}
</head>

<body class="page-header-fixed bg-bgcolor" style="overflow-y:auto;position:relative;">
<!-- BEGIN HEADER -->
<div class="header navbar navbar-inverse navbar-fixed-top">
    <!-- BEGIN TOP NAVIGATION BAR -->
    <div class="navbar-inner">
        <div class="container-fluid">
            <!-- BEGIN LOGO -->
            <a class="brand logo" href="/index.do">
                 <img src="/image/ilogo.png" alt="logo" />
            </a>
            <!-- END LOGO -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="btn-navbar collapsed" data-toggle="collapse" data-target=".nav-collapse"> <img
                    src="image/menu-toggler.png" alt=""/> </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            {{!--<ul class="nav navbar-nav pull-right">
                <li>{{roleName}}</li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        {{userName}}
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu">
                        <li><a href="/loginOut" id="login_out">退出</a></li>
                    </ul>
                </li>
            </ul>--}}
        </div>
    </div>
    <!-- END TOP NAVIGATION BAR -->
</div>
<!-- END HEADER -->
<!-- BEGIN CONTAINER -->
<div class="page-container row-fluid">
    {{>side_menu}}
    <!-- BEGIN PAGE -->
    <div class="page-content" style="height:100%;">
        <!-- BEGIN PAGE CONTAINER-->
        <div class="container-fluid">
            <!-- BEGIN PAGE HEADER-->
            <div class="row-fluid">
                <div class="span12">
                    <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                    {{{_sections.pagetitle}}}
                    <!-- END PAGE TITLE & BREADCRUMB-->
                </div>
            </div>
            {{{body}}}
        </div>
    </div>
</div>
<!-- BEGIN CORE PLUGINS -->
<script src="js/jquery.min.js" type="text/javascript"></script>
<script src="js/bootstrap.min.js" type="text/javascript"></script>
<script src="js/echarts.min.js" type="text/javascript"></script>
<script src="js/layer/layer.js" type="text/javascript"></script>
<script src="js/jquery.cookie.min.js" type="text/javascript"></script>
<script src="js/app.js"></script>
<script src="js/jquery.mCustomScrollbar.concat.min.js"></script>
<script src="js/configuration.js"></script>
<script src="js/index.js"></script>
<script>
    jQuery(document).ready(function () {
        App.init();
    });
</script>
{{{_sections.js}}}
